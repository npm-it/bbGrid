{
    "name": "npmweb/bbgrid",
    "description": "An extendable grid system (jqGrid like) developed on Backbone.js",
    "type": "component",
    "require": {
        "robloach/component-installer": "*",
        "components/backbone": "1.1.*"
    },
    "extra": {
        "component": {
            "scripts": [
                "bbGrid.js"
            ],
            "styles": [
                "css/bbGrid.css"
            ],
            "shim": {
                "deps": ["backbone", "jquery"],
                "exports": "bbGrid"
            }
        }
    }
}